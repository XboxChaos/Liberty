                             ScrollViewer.VerticalScrollBarVisibility="Auto" Background="{x:Null}" BorderBrush="#FF3C3C3C" 
                             Foreground="{x:Null}" MouseDoubleClick="fatxSaveSelector_MouseDoubleClick">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Grid Height="100" HorizontalAlignment="Stretch">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="250" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    
                                    <Image Grid.Column="0" Margin="5" Source="{Binding MapImage}" />
                                    
                                    <Grid Grid.Column="1">
                                        <Grid Margin="10,0,0,0">
                                            <StackPanel>
                                                <TextBlock Text="{Binding GamerTag}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,5,0,0" FontFamily="/Liberty;component/Fonts/#Segoe WP SemiLight" FontSize="20" Foreground="White" />
                                                <TextBlock Text="{Binding MapName}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,5,0,0" FontFamily="/Liberty;component/Fonts/#Segoe WP SemiLight" FontSize="18" Foreground="White" />
                                            </StackPanel>
                                            <TextBlock Text="{Binding GameName}" FontFamily="/Liberty;component/Fonts/#Segoe WP Light" Foreground="#FF999999" FontSize="72" Margin="0,19,0,0" Opacity="0.1" />
                                        </Grid>
                                        <Grid HorizontalAlignment="Right">
                                            
                                        </Grid>
                                    </Grid>
                                    
                                </Grid>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                    <WP7:ProgressionBar x:Name="progressPendingRefresh" Visibility="Collapsed" Height="3" VerticalAlignment="Top" Margin="0,7,40,0" Foreground="{x:Null}" ProgressColour="{DynamicResource LibertyAccentBrushSecondary}" />

                    <Button x:Name="btnRefresh"  Content="Refresh" Margin="0,0,40,71" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="77" Click="btnRefresh_Click" />
                    <Button x:Name="btnBack" Content="Back" Margin="0,0,578,71" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="77" Click="btnBack_Click" />
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
